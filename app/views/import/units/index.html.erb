<div class="container">
  <div class="row">
    <h1>Upload external data file</h1>
    <div class="col-6">
      <%= form_tag({ url: import_units_path }, multipart: true) do %>
        <div class="input-group">
          <%= file_field_tag :file, class: "form-control" %>
          <%= submit_tag "Upload", class: "btn btn-primary" %>
        </div>
      <% end %>
    </div>

    <div class="col-6"></div>
  </div>
  <br />
  <div class="row">
    <div class="col-6">
      <%= link_to "Back to homepage", root_path, class: "btn btn-secondary" %>
    </div>
  </div>
</div>
